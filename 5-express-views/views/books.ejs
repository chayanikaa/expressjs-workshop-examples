<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Books</title>
</head>
<body>
  <ul>
    <h1>All Books</h1>
    <% books.forEach(function(book){ %>
      <li><%- book.title %></li>
    <% }); %>
  </ul>
</body>
</html>